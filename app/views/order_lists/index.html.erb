<p id="notice"><%= notice %></p>

<h2>Состав заказа</h2>

<table>
  <thead>
    <tr>
      <th>Заказ:</th>
      <th>Услуга:</th>
      <th>Выполнил:</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_lists.map do |order_list| %>
      <tr>
        <td><%= order_list.order.car %></td>
        <td><%= order_list.service.name %></td>
        <td><%= order_list.specialist.name %></td>
        <td><%= link_to 'Show', order_list_path(order_list.id) %></td>
        <td><%= link_to 'Edit', edit_order_list_path(order_list.id) %></td>
        <td><%= link_to 'Destroy', order_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order List', new_order_list_path %>
